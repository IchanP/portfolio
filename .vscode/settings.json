{
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "[typescript]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescriptreact]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "files.eol": "\n",
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "i18n-ally.localesPaths": [
      "messages",
      "src/i18n"
    ],
    "i18n-ally.keystyle": "nested"
  }
  